{
  "name": "mailpy-common",
  "version": "1.0.0",
  "description": "Mailpy Common",
  "author": "Claudio Carneiro",
  "license": "GPL-3.0",
  "main": "dist/src/index.js",
  "scripts": {
    "prepare": "npm run build-ts-es",
    "build-ts-cjs": "npx tsc --build --verbose tsconfig-cjs.json",
    "build-ts-es": "npx tsc --build --verbose tsconfig.json",
    "clean": "npm run clean-es && npm run clean-cjs",
    "clean-es": "tsc --build --clean tsconfig.json",
    "clean-cjs": "tsc --build --clean tsconfig-cjs.json",
    "build": "npm run test && npm run clean && npm run build-ts-es && npm run build-ts-cjs",
    "test": "npx jest --detectOpenHandles src",
    "test-watch": "npx jest --watch --verbose --detectOpenHandles src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carneirofc/mailpy-api.git"
  },
  "bugs": {
    "url": "https://github.com/carneirofc/mailpy-api/issues"
  },
  "homepage": "https://github.com/carneirofc/mailpy-api#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/faker": "^5.5.6",
    "@types/jest": "^26.0.20",
    "@types/node": "^16.4.1",
    "eslint": "^7.29.0",
    "faker": "^5.1.0",
    "jest": "^27.0.5",
    "jest-environment-node": "^26.6.2",
    "prettier": "^2.3.1",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5"
  }
}
